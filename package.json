{
  "name": "@oobe-protocol-labs/synapse-client-sdk",
  "version": "1.0.2",
  "description": "Synapse Client SDK -- Typed Solana RPC gateway with @solana/kit native types, 53 JSON-RPC methods, DAS, WebSocket, gRPC/Geyser parser, signers/wallets, and AI agent tooling (LangChain, x402) | Powered by oobeprotocol.ai (OOBE)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./core": {
      "types": "./dist/esm/core/client.d.ts",
      "import": "./dist/esm/core/client.js",
      "require": "./dist/cjs/core/client.js"
    },
    "./core/types": {
      "types": "./dist/esm/core/types.d.ts",
      "import": "./dist/esm/core/types.js",
      "require": "./dist/cjs/core/types.js"
    },
    "./rpc": {
      "types": "./dist/esm/rpc/index.d.ts",
      "import": "./dist/esm/rpc/index.js",
      "require": "./dist/cjs/rpc/index.js"
    },
    "./grpc": {
      "types": "./dist/esm/grpc/index.d.ts",
      "import": "./dist/esm/grpc/index.js",
      "require": "./dist/cjs/grpc/index.js"
    },
    "./das": {
      "types": "./dist/esm/das/index.d.ts",
      "import": "./dist/esm/das/index.js",
      "require": "./dist/cjs/das/index.js"
    },
    "./websocket": {
      "types": "./dist/esm/websocket/index.d.ts",
      "import": "./dist/esm/websocket/index.js",
      "require": "./dist/cjs/websocket/index.js"
    },
    "./utils": {
      "types": "./dist/esm/utils/index.d.ts",
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.js"
    },
    "./ai": {
      "types": "./dist/esm/ai/index.d.ts",
      "import": "./dist/esm/ai/index.js",
      "require": "./dist/cjs/ai/index.js"
    },
    "./kit": {
      "types": "./dist/esm/kit/index.d.ts",
      "import": "./dist/esm/kit/index.js",
      "require": "./dist/cjs/kit/index.js"
    },
    "./decoders": {
      "types": "./dist/esm/decoders/index.d.ts",
      "import": "./dist/esm/decoders/index.js",
      "require": "./dist/cjs/decoders/index.js"
    },
    "./accounts": {
      "types": "./dist/esm/accounts/index.d.ts",
      "import": "./dist/esm/accounts/index.js",
      "require": "./dist/cjs/accounts/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "browser": {
    "ws": false,
    "@grpc/grpc-js": false,
    "@grpc/proto-loader": false
  },
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && node scripts/fix-esm-imports.mjs && pnpm run -s build:docs",
    "build:docs": "typedoc --out dist/docs src/index.ts && cp -r dist/docs docs && touch docs/.nojekyll",
    "docs": "typedoc --out docs src/index.ts",
    "win-build": "rimraf dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && pnpm run -s win-build:docs",
    "win-build:docs": "typedoc --out dist/docs src/index.ts || echo 'typedoc not found, skipping docs build'",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "benchmark": "node benchmark/performance.mjs",
    "stress-test": "node benchmark/stress-test.mjs"
  },
  "keywords": [
    "solana",
    "solana-kit",
    "rpc",
    "gateway",
    "client",
    "sdk",
    "signers",
    "wallet",
    "ai",
    "agents",
    "langchain",
    "typescript",
    "websocket",
    "grpc",
    "geyser",
    "das-api",
    "compressed-nft",
    "x402",
    "blockchain",
    "web3",
    "json-rpc",
    "zod"
  ],
  "author": "Synapse Gateway | OOBE PROTOCOL LABS (https://oobeprotocol.ai)",
  "license": "MIT",
  "dependencies": {
    "@solana/kit": "^6.1.0",
    "eventemitter3": "^5.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@langchain/core": "^0.3.78",
    "@types/node": "^24.3.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.33.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typedoc": "^0.26.10",
    "typescript": "^5.9.2",
    "vitest": "^4.0.18",
    "zod": "^4.1.11"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.3.0 <0.4.0",
    "typescript": ">=5.0.0",
    "zod": ">=3.23.0 || >=4.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    },
    "@langchain/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OOBE-PROTOCOL/synapse-client-sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
